   "source": [
    "### 3.1 Conversion by Age Group"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Analyze conversion by age group\n",
    "age_analysis = df.groupby('age_group').agg({\n",
    "    'converted': ['sum', 'count', 'mean']\n",
    "})\n",
    "age_analysis.columns = ['conversions', 'total_contacts', 'conversion_rate']\n",
    "age_analysis['conversion_rate'] = (age_analysis['conversion_rate'] * 100).round(2)\n",
    "\n",
    "print(age_analysis)\n",
    "\n",
    "# Visualize\n",
    "plt.figure(figsize=(10, 6))\n",
    "bars = plt.bar(age_analysis.index, age_analysis['conversion_rate'], \n",
    "               color=sns.color_palette('viridis', len(age_analysis)), edgecolor='black')\n",
    "plt.xlabel('Age Group', fontsize=12, fontweight='bold')\n",
    "plt.ylabel('Conversion Rate (%)', fontsize=12, fontweight='bold')\n",
    "plt.title('Conversion Rate by Age Group', fontsize=14, fontweight='bold')\n",
    "\n",
    "for bar in bars:\n",
    "    height = bar.get_height()\n",
    "    plt.text(bar.get_x() + bar.get_width()/2., height,\n",
    "            f'{height:.1f}%', ha='center', va='bottom', fontweight='bold')\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "### 3.2 Channel Performance Analysis"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Analyze performance by channel\n",
    "channel_analysis = df.groupby('contact').agg({\n",
    "    'converted': ['sum', 'count', 'mean']\n",
    "})\n",
    "channel_analysis.columns = ['conversions', 'total_contacts', 'conversion_rate']\n",
    "channel_analysis['conversion_rate'] = (channel_analysis['conversion_rate'] * 100).round(2)\n",
    "\n",
    "print(channel_analysis)\n",
    "\n",
    "# Visualize\n",
    "fig, (ax1, ax2) = plt.subplots(1, 2, figsize=(14, 6))\n",
    "\n",
    "# Volume by channel\n",
    "channel_analysis['total_contacts'].plot(kind='bar', ax=ax1, color='skyblue', edgecolor='black')\n",
    "ax1.set_title('Campaign Volume by Channel', fontsize=13, fontweight='bold')\n",
    "ax1.set_ylabel('Total Contacts', fontsize=11)\n",
    "ax1.set_xlabel('Channel', fontsize=11)\n",
    "\n",
    "# Conversion rate by channel\n",
    "channel_analysis['conversion_rate'].plot(kind='bar', ax=ax2, color='coral', edgecolor='black')\n",
    "ax2.set_title('Conversion Rate by Channel', fontsize=13, fontweight='bold')\n",
    "ax2.set_ylabel('Conversion Rate (%)', fontsize=11)\n",
    "ax2.set_xlabel('Channel', fontsize=11)\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "### 3.3 Campaign Contact Frequency"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Analyze optimal contact frequency\n",
    "campaign_bins = [0, 1, 2, 3, 5, 10, 100]\n",
    "campaign_labels = ['1', '2', '3', '4-5', '6-10', '10+']\n",
    "df['campaign_group'] = pd.cut(df['campaign'], bins=campaign_bins, \n",
    "                              labels=campaign_labels, include_lowest=True)\n",
    "\n",
    "campaign_analysis = df.groupby('campaign_group').agg({\n",
    "    'converted': ['sum', 'count', 'mean']\n",
    "})\n",
    "campaign_analysis.columns = ['conversions', 'total_contacts', 'conversion_rate']\n",
    "campaign_analysis['conversion_rate'] = (campaign_analysis['conversion_rate'] * 100).round(2)\n",
    "\n",
    "print(campaign_analysis)\n",
    "\n",
    "# Visualize\n",
    "plt.figure(figsize=(12, 6))\n",
    "bars = plt.bar(campaign_analysis.index, campaign_analysis['conversion_rate'],\n",
    "               color=sns.color_palette('rocket', len(campaign_analysis)), edgecolor='black')\n",
    "plt.xlabel('Number of Contacts', fontsize=12, fontweight='bold')\n",
    "plt.ylabel('Conversion Rate (%)', fontsize=12, fontweight='bold')\n",
    "plt.title('Campaign Effectiveness by Contact Frequency', fontsize=14, fontweight='bold')\n",
    "\n",
    "for i, bar in enumerate(bars):\n",
    "    height = bar.get_height()\n",
    "    count = campaign_analysis.iloc[i]['total_contacts']\n",
    "    plt.text(bar.get_x() + bar.get_width()/2., height,\n",
    "            f'{height:.1f}%\\n(n={count:,})', ha='center', va='bottom', fontsize=9)\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## 4. KPI Calculation"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Define assumptions\n",
    "COST_PER_CONTACT = 50  # dollars\n",
    "AVG_CUSTOMER_VALUE = 1000  # dollars\n",
    "RETENTION_RATE = 0.75\n",
    "DISCOUNT_RATE = 0.10\n",
    "\n",
    "# Calculate KPIs\n",
    "total_marketing_spend = len(df) * COST_PER_CONTACT\n",
    "customers_acquired = df['converted'].sum()\n",
    "revenue = customers_acquired * AVG_CUSTOMER_VALUE\n",
    "\n",
    "# CAC - Customer Acquisition Cost\n",
    "cac = total_marketing_spend / customers_acquired\n",
    "\n",
    "# ROI - Return on Investment\n",
    "roi = ((revenue - total_marketing_spend) / total_marketing_spend * 100)\n",
    "\n",
    "# CLV - Customer Lifetime Value\n",
    "clv = (AVG_CUSTOMER_VALUE * RETENTION_RATE) / (1 + DISCOUNT_RATE - RETENTION_RATE)\n",
    "\n",
    "# CLV to CAC Ratio\n",
    "clv_cac_ratio = clv / cac\n",
    "\n",
    "print(\"=\"*50)\n",
    "print(\"KEY PERFORMANCE INDICATORS\")\n",
    "print(\"=\"*50)\n",
    "print(f\"\\nTotal Marketing Spend: ${total_marketing_spend:,.2f}\")\n",
    "print(f\"Customers Acquired: {customers_acquired:,}\")\n",
    "print(f\"Revenue Generated: ${revenue:,.2f}\")\n",
    "print(f\"\\nCustomer Acquisition Cost (CAC): ${cac:,.2f}\")\n",
    "print(f\"Customer Lifetime Value (CLV): ${clv:,.2f}\")\n",
    "print(f\"Return on Investment (ROI): {roi:.2f}%\")\n",
    "print(f\"CLV to CAC Ratio: {clv_cac_ratio:.2f}x\")\n",
    "print(f\"\\nConversion Rate: {conversion_rate:.2f}%\")"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## 5. Key Insights and Recommendations"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Generate insights\n",
    "age_conv = df.groupby('age_group')['converted'].mean() * 100\n",
    "best_age = age_conv.idxmax()\n",
    "best_age_rate = age_conv.max()\n",
    "multiplier = best_age_rate / conversion_rate\n",
    "\n",
    "channel_conv = df.groupby('contact')['converted'].mean() * 100\n",
    "best_channel = channel_conv.idxmax()\n",
    "best_channel_rate = channel_conv.max()\n",
    "\n",
    "print(\"=\"*60)\n",
    "print(\"KEY INSIGHTS & RECOMMENDATIONS\")\n",
    "print(\"=\"*60)\n",
    "\n",
    "print(f\"\\n1. AGE GROUP TARGETING:\")\n",
    "print(f\"   • The {best_age} age group has the highest conversion rate: {best_age_rate:.2f}%\")\n",
    "print(f\"   • This is {multiplier:.2f}x higher than the overall average\")\n",
    "print(f\"   • RECOMMENDATION: Allocate 40-50% of budget to {best_age} demographic\")\n",
    "\n",
    "print(f\"\\n2. CHANNEL OPTIMIZATION:\")\n",
    "print(f\"   • {best_channel.capitalize()} channel shows best performance: {best_channel_rate:.2f}%\")\n",
    "print(f\"   • RECOMMENDATION: Prioritize {best_channel} for future campaigns\")\n",
    "\n",
    "optimal_contacts = campaign_analysis['conversion_rate'].idxmax()\n",
    "print(f\"\\n3. CONTACT FREQUENCY:\")\n",
    "print(f\"   • Optimal contact frequency: {optimal_contacts} times\")\n",
    "print(f\"   • RECOMMENDATION: Limit contacts to avoid diminishing returns\")\n",
    "\n",
    "print(f\"\\n4. FINANCIAL HEALTH:\")\n",
    "if clv_cac_ratio > 3:\n",
    "    print(f\"   • CLV/CAC Ratio of {clv_cac_ratio:.2f}x indicates EXCELLENT performance\")\n",
    "    print(f\"   • RECOMMENDATION: Scale up successful campaigns\")\n",
    "else:\n",
    "    print(f\"   • CLV/CAC Ratio of {clv_cac_ratio:.2f}x needs improvement\")\n",
    "    print(f\"   • RECOMMENDATION: Focus on reducing CAC through better targeting\")\n",
    "\n",
    "print(f\"\\n5. ROI ANALYSIS:\")\n",
    "print(f\"   • Current ROI: {roi:.2f}%\")\n",
    "if roi > 100:\n",
    "    print(f\"   • STATUS: Strong positive return on marketing investment\")\n",
    "else:\n",
    "    print(f\"   • STATUS: Needs optimization to improve profitability\")"
   ]
  },
